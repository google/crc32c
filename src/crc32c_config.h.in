#ifndef CRC32C_CRC32C_CONFIG_H_
#define CRC32C_CRC32C_CONFIG_H_

/* Define to 1 if building for a big-endian platform. */
#cmakedefine BYTE_ORDER_BIG_ENDIAN 1

/* Define to 1 if the compiler has the _mm_crc32_u{8,32,64} intrinsics. */
#cmakedefine HAVE_SSE42 1

/* Define to 1 if the compiler has the __crc32cd and vmull_p64 intrinsics, and
   the getauxval function in the <sys/auxv.h> header. The latter should be true
   on Linux. */
#cmakedefine HAVE_ARM_LINUX_CRC32C 1

/* Define to 1 if CRC32C tests have been built with Google Logging. */
#cmakedefine CRC32C_TESTS_BUILT_WITH_GLOG 1

#endif  // CRC32C_CRC32C_CONFIG_H_
